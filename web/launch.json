{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Web (Chrome)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "--web-renderer",
        "html"
      ],
      "webBrowser": {
        "name": "chrome",
        "args": [
          "--disable-web-security",
          "--disable-features=VizDisplayCompositor",
          "--user-data-dir=/tmp/chrome_dev_test"
        ]
      }
    }
  ]
}
